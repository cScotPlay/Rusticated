{
  "schemaVersion": 1,
  "id": "rusticated",
  "version": "${version}",

  "name": "Rusticated",
  "description": "Rusticated is an updated and reinvigorated mod based on our beloved, Rustic.",
  "authors": [
    "cScotPlay",
    "cadaverous-eris (Original Mastermind)"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/rusticated",
    "sources": "https://github.com/cScotPlay/Rusticated",
    "issues": "https://github.com/cScotPlay/Rusticated/issues"
  },

  "license": "MIT",
  "icon": "assets/rusticated/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.mcs3.rusticated.Rusticated"
    ],
    "client": [
      "net.mcs3.rusticated.RusticatedClient"
    ],
    "fabric-datagen": [
      "net.mcs3.rusticated.data.DataGenerators"
    ],
    "jei_mod_plugin": [
      "net.mcs3.rusticated.compat.jei.RusticatedJEIPlugin"
    ],
    "rei_client": [
      "net.mcs3.rusticated.compat.rei.RusticatedREIPlugin"
    ]
  },
  "mixins": [
    "rusticated.mixins.json"
  ],
  
	"depends": {
		"fabricloader": ">=0.14.22",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}