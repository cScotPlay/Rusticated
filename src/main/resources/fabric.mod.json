{
  "schemaVersion": 1,
  "id": "rusticated",
  "version": "${version}",

  "name": "Rusticated",
  "description": "Rusticated is the newest and reinvigorate version of our beloved mod Rustic.",
  "authors": [
    "cScotP",
    "cadaverous-eris (Original Mastermind)"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/rusticated",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "MIT",
  "icon": "assets/rusticated/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.mcs3.rusticated.Rusticated"
    ],
    "client": [
      "net.mcs3.rusticated.RusticatedClient"
    ],
    "fabric-datagen": [
      "net.mcs3.rusticated.data.DataGenerators"
    ],
    "jei_mod_plugin": [
      "net.mcs3.rusticated.compat.jei.RusticatedJEIPlugin"
    ]
  },
  "mixins": [
    "rusticated.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.18.2",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
